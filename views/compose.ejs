<%- include('partials/header'); -%> 

<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="textBox1">Title</label>
        <input class="form-control form-control-sm" type="text" name="postTitle" id="textBox1">
    </div>
    <div class="form-group">
        <label for="textBox2">Post</label>
        <textarea class="form-control" rows="5" name="postBody" id="textBox2"></textarea>
    </div>
    <button class="btn btn-primary" type="submit">Publish</button>
</form>

<%- include('partials/footer'); -%>